{
	"name": "leadforgeai-authorization-module",
	"version": "0.1.0",
	"description": "Minimal Express + Effect functional shell/core scaffold with Drizzle schemas.",
	"type": "module",
	"scripts": {
		"dev": "tsx src/app/server.ts",
		"dev:neon": "node scripts/run-dev-with-development-db.js",
		"build": "vite build",
		"lint": "npx @ton-ai-core/vibecode-linter src",
		"lint-frontend": "npx @ton-ai-core/vibecode-linter react-admin",
		"build:schema": "tsc -p tsconfig.schema.json",
		"test": "vitest run",
		"drizzle:generate": "npm run build:schema && drizzle-kit generate",
		"drizzle:push": "npm run build:schema && drizzle-kit push"
	},
	"dependencies": {
		"@effect/schema": "^0.67.24",
		"@effect/sql": "^0.48.6",
		"@effect/sql-drizzle": "^0.47.0",
		"@effect/sql-pg": "^0.49.7",
		"@handfish/drizzle-effect": "^0.0.17",
		"@types/bcryptjs": "^2.4.6",
		"@types/express-session": "^1.18.2",
		"@types/swagger-ui-express": "^4.1.8",
		"bcryptjs": "^3.0.3",
		"dotenv": "^16.4.5",
		"drizzle-orm": "^0.44.7",
		"effect": "^3.19.8",
		"eslint-plugin-drizzle": "^0.2.3",
		"express": "^4.18.2",
		"express-session": "^1.18.2",
		"node-fetch": "^3.3.2",
		"pg": "^8.11.5",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"resend": "^3.2.0",
		"swagger-ui-express": "^5.0.1",
		"ts-pattern": "^5.3.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.3",
		"@effect/eslint-plugin": "^0.3.2",
		"@effect/language-service": "^0.57.1",
		"@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
		"@eslint/js": "^9.39.1",
		"@ton-ai-core/eslint-plugin-suggest-members": "^1.0.3",
		"@ton-ai-core/vibecode-linter": "^1.0.6",
		"@types/express": "^4.17.21",
		"@types/pg": "^8.15.6",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"drizzle-kit": "^0.31.7",
		"eslint": "^9.39.1",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-sql": "^3.2.2",
		"eslint-plugin-sql-template": "^3.1.0",
		"eslint-plugin-sqlite": "^1.6.0",
		"eslint-plugin-typeorm-typescript": "^0.5.1",
		"eslint-plugin-vitest": "^0.5.4",
		"globals": "^16.5.0",
		"ts-node": "^10.9.2",
		"tslib": "^2.6.2",
		"tsx": "^4.7.1",
		"typescript": "^5.4.2",
		"typescript-eslint": "^8.46.3",
		"vite": "^7.2.4",
		"vitest": "^1.5.0"
	}
}
